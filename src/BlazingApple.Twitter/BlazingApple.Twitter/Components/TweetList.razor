@using BlazingApple.Twitter.Models
@if (TwitterId is not null)
{
    <TwitterHostedList Id="@TwitterId" @ref="_embeddedList" Type="TimelineType" HeightInPixels="HeightInPixels" MaxCount="MaxCount"/>
}
else if(Tweets is not null)
{
    @foreach(Tweet tweet in Tweets)
    {
        <TweetDisplay Tweet="tweet"></TweetDisplay>
    }
}